<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<HBox xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <children>
            <Pane>
               <children>
                  <VBox spacing="9.0">
                     <children>
                        <Label text="Tattui">
                           <font>
                              <Font name="Times New Roman" size="30.0" />
                           </font>
                        </Label>
                        <Label text="Welcome to Tattui, your hub for tattoo things" textFill="#514d46">
                           <font>
                              <Font name="Times New Roman" size="12.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="6.0" top="6.0" />
                           </VBox.margin>
                        </Label>
                        <Label lineSpacing="10.0" text="New Project" textFill="#23a5b9">
                           <font>
                              <Font name="Times New Roman" size="15.0" />
                           </font>
                        </Label>
                        <Label lineSpacing="10.0" text="Upload Files" textFill="#23a5b9">
                           <font>
                              <Font name="Times New Roman" size="15.0" />
                           </font>
                        </Label>
                        <Label lineSpacing="10.0" text="Inspiration" textFill="#23a5b9">
                           <font>
                              <Font name="Times New Roman" size="15.0" />
                           </font>
                        </Label>
                        <Region prefHeight="94.0" prefWidth="200.0" />
                     </children>
                  </VBox>
               </children>
            </Pane>
            <VBox>
               <children>
                  <Label text="Recents">
                     <font>
                        <Font name="Times New Roman" size="24.0" />
                     </font>
                  </Label>
                  <HBox>
                     <children>
                        <VBox prefHeight="200.0" prefWidth="100.0">
                           <children>
                              <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                              <Label text="Unnamed obj">
                                 <font>
                                    <Font name="Times New Roman" size="12.0" />
                                 </font>
                              </Label>
                              <Label text="Open Project" textFill="#23a5b9">
                                 <font>
                                    <Font name="Times New Roman" size="10.0" />
                                 </font>
                                 <VBox.margin>
                                    <Insets bottom="30.0" top="5.0" />
                                 </VBox.margin>
                              </Label>
                           </children>
                        </VBox>
                        <VBox>
                           <children>
                              <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                              <Label text="wow so empty, maybe something more?">
                                 <font>
                                    <Font name="Times New Roman" size="12.0" />
                                 </font>
                              </Label>
                           </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
         <HBox.margin>
            <Insets left="20.0" />
         </HBox.margin>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="Tutorials">
               <font>
                  <Font name="Times New Roman" size="18.0" />
               </font>
            </Label>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label text="Making Your First Project" textFill="#23a5b9">
                     <font>
                        <Font name="Times New Roman" size="12.0" />
                     </font>
                  </Label>
                  <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Label text="How to pick a spot for your tattoo" textFill="#23a5b9">
                     <font>
                        <Font name="Times New Roman" size="12.0" />
                     </font>
                  </Label>
                  <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets top="10.0" />
   </padding>
</HBox>
