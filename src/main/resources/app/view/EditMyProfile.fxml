<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>

<VBox prefHeight="856.0" prefWidth="1095.0" stylesheets="@../css/main.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.boundary.EditArtistProfileBoundary">
   <children>
      <BorderPane prefHeight="51.0" style="-fx-border-color: white; -fx-border-width: 0 0 1 0;">
         <center>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="709.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="Title" text="Edit My Profile" />
               </children>
            </HBox>
         </center>
      </BorderPane>
      <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <children>
            <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <top>
                  <VBox alignment="CENTER" BorderPane.alignment="CENTER">
                     <children>
                        <HBox alignment="CENTER" prefHeight="118.0" prefWidth="528.0" spacing="25.0" stylesheets="@../css/main.css">
                           <children>
                              <Circle fx:id="profilePicture" fill="DODGERBLUE" onMouseClicked="#handleProfileClick" radius="50.0" stroke="BLACK" strokeType="INSIDE" />
                              <VBox alignment="CENTER_LEFT" prefHeight="111.0" prefWidth="390.0" spacing="5.0">
                                 <children>
                                    <Label fx:id="artistNameField" styleClass="title" text="Artist Name" />
                                    <TextArea fx:id="biographyField" prefHeight="200.0" prefWidth="200.0" promptText="Write Something About yourself" text="This is a bio message left by the artist, more words are used to show that this can take up extra space." wrapText="true" />
                                    <HBox alignment="CENTER" spacing="5.0">
                                       <children>
                                          <TextField fx:id="latitudeField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" promptText="Latitude" HBox.hgrow="ALWAYS" />
                                          <TextField fx:id="longitudeField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" promptText="Longitude  " HBox.hgrow="ALWAYS" />
                                       </children>
                                    </HBox>
                                 </children>
                              </VBox>
                           </children>
                           <padding>
                              <Insets left="10.0" top="25.0" />
                           </padding>
                        </HBox>
                        <HBox alignment="CENTER">
                           <children>
                              <BorderPane prefHeight="55.0" prefWidth="303.0">
                                 <left>
                                    <HBox alignment="CENTER" prefHeight="35.0" prefWidth="40.0" BorderPane.alignment="CENTER">
                                       <children>
                                          <Label text="Styles: " />
                                          <Label fx:id="styles" />
                                       </children>
                                       <BorderPane.margin>
                                          <Insets bottom="20.0" top="20.0" />
                                       </BorderPane.margin>
                                    </HBox>
                                 </left>
                                 <center>
                                    <ComboBox fx:id="styleComboBox" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="20.0" prefWidth="150.0" BorderPane.alignment="CENTER" />
                                 </center>
                                 <right>
                                    <HBox alignment="CENTER" fillHeight="false" prefHeight="35.0" prefWidth="20.0" spacing="10.0" BorderPane.alignment="CENTER">
                                       <children>
                                          <Button fx:id="addTagButton" minWidth="28.0" mnemonicParsing="false" onAction="#addTag" prefHeight="28.0" prefWidth="28.0" text="+" HBox.hgrow="NEVER" />
                                          <Button fx:id="deleteTagButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="30.0" mnemonicParsing="false" onAction="#deleteTag" prefHeight="28.0" text="-" HBox.hgrow="NEVER" />
                                       </children>
                                       <BorderPane.margin>
                                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                       </BorderPane.margin>
                                    </HBox>
                                 </right>
                                 <HBox.margin>
                                    <Insets />
                                 </HBox.margin>
                              </BorderPane>
                           </children>
                        </HBox>
                        <HBox alignment="CENTER">
                           <children>
                              <HBox alignment="CENTER" spacing="20.0">
                                 <children>
                                    <Button mnemonicParsing="false" onAction="#handleSaveChanges" prefHeight="31.0" prefWidth="143.0" text="Save Profile" />
                                    <Button mnemonicParsing="false" onAction="#addPost" prefHeight="31.0" prefWidth="143.0" text="Add Post" />
                                 </children>
                                 <opaqueInsets>
                                    <Insets />
                                 </opaqueInsets>
                                 <padding>
                                    <Insets bottom="10.0" top="10.0" />
                                 </padding>
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin>
                              </HBox>
                           </children>
                        </HBox>
                     </children>
                     <BorderPane.margin>
                        <Insets bottom="25.0" top="25.0" />
                     </BorderPane.margin>
                  </VBox>
               </top>
               <center>
                 <ScrollPane fx:id="scrollPane" fitToWidth="true" hbarPolicy="NEVER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-border-color: white; -fx-border-width: 1 0 0 0;" vbarPolicy="NEVER">
                     <content>
                         <GridPane fx:id="postsPanel" alignment="CENTER" gridLinesVisible="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="-Infinity">
                                 <columnConstraints>
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="289.0" minWidth="10.0" prefWidth="286.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="296.0" minWidth="10.0" prefWidth="292.0" />
                                    <ColumnConstraints hgrow="SOMETIMES" maxWidth="296.0" minWidth="10.0" prefWidth="292.0" />
                                 </columnConstraints>
                              </GridPane>
                     </content>
                     <BorderPane.margin>
                        <Insets />
                     </BorderPane.margin>
                  </ScrollPane>
               </center>
            </BorderPane>
         </children>
      </AnchorPane>
   </children>
</VBox>
